{
  "name": "boorie",
  "version": "1.2.0",
  "description": "Boorie - Advanced AI Desktop Client with comprehensive productivity features",
  "main": "dist/electron/main.js",
  "scripts": {
    "dev": "node scripts/dev-runner.js",
    "dev:vite": "vite",
    "dev:electron": "npm run build:electron-ts && wait-on http://localhost:3000 && electron .",
    "build": "npm run build:vite && npm run build:electron",
    "build:vite": "vite build",
    "build:electron": "npm run build:electron-ts && electron-builder",
    "build:electron-ts": "tsc -p tsconfig.electron.json && node scripts/copy-assets.js",
    "build:app": "electron-builder",
    "dist": "npm run build && npm run build:app",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "electron",
    "ai",
    "desktop",
    "productivity"
  ],
  "author": "AI Desktop Client Team",
  "license": "MIT",
  "devDependencies": {
    "@prisma/client": "^6.19.0",
    "@prisma/engines": "^6.10.1",
    "@types/node": "^20.11.17",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.35",
    "prisma": "^6.19.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.0",
    "wait-on": "^7.2.0"
  },
  "overrides": {
    "esbuild": "^0.25.0"
  },
  "dependencies": {
    "@langchain/community": "^1.0.7",
    "@langchain/core": "^1.1.4",
    "@langchain/ollama": "^1.0.3",
    "@langchain/openai": "^1.1.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@types/mapbox-gl": "^3.4.1",
    "axios": "^1.12.2",
    "chart.js": "^4.5.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^4.1.0",
    "electron-log": "^5.0.1",
    "electron-updater": "^6.1.8",
    "framer-motion": "^11.0.3",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "langchain": "^1.1.5",
    "lucide-react": "^0.323.0",
    "mammoth": "^1.11.0",
    "mapbox-gl": "^3.15.0",
    "pdf-parse": "^1.1.1",
    "proj4": "^2.19.10",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.5.3",
    "tailwind-merge": "^2.2.1",
    "vis-network": "^10.0.2",
    "zustand": "^4.5.0"
  },
  "build": {
    "appId": "com.boorie.app",
    "productName": "Boorie",
    "icon": "resources/icon",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "resources/**/*",
      "prisma/**/*"
    ],
    "extraResources": [
      {
        "from": "prisma",
        "to": "prisma",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "node_modules/.prisma",
        "to": ".prisma",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "node_modules/@prisma/client",
        "to": "@prisma/client",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "node_modules/@prisma/client/runtime",
        "to": "@prisma/client/runtime",
        "filter": [
          "**/*"
        ]
      }
    ],
    "asarUnpack": [
      "node_modules/@prisma/**/*",
      "node_modules/.prisma/**/*",
      "node_modules/axios/**/*"
    ],
    "mac": {
      "target": "dmg",
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  }
}